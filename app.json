{
  "id": "se.warting.nfctools",
  "permissions": [
    "homey:wireless:nfc"
  ],
  "sdk": 2,
  "brandColor": "#FF0000",
  "name": {
    "en": "NFC tools"
  },
  "description": {
    "en": "NFC tools"
  },
  "images": {
    "large": "/assets/images/large.png",
    "small": "/assets/images/small.png"
  },
  "category": "tools",
  "version": "1.0.2",
  "compatibility": ">=1.5.0",
  "author": {
    "name": "Stefan Wärting",
    "email": "stefan@warting.se"
  },
  "contributors": {
    "developers": [
      {
        "name": "Stefan Wärting",
        "email": "stefan@warting.se"
      },
      {
        "name": "Jimmy Wärting",
        "email": "jimmy@warting.se"
      }
    ]
  },
  "contributing": {
    "donate": {
      "paypal": {
        "username": "warting"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/warting/nfc-tools/issues"
  },
  "flow": {
    "triggers": [
      {
        "id": "nfc_touched",
        "title": {
          "en": "NFC touched"
        },
        "tokens": [
          {
            "name": "data",
            "type": "string",
            "title": {
              "en": "Data"
            },
            "example": "https://www.youtube.com/watch?v=vFFDArGuLBg"
          },
          {
            "name": "title",
            "type": "string",
            "title": {
              "en": "Title"
            },
            "example": "Movie title"
          },
          {
            "name": "id",
            "type": "string",
            "title": {
              "en": "Id"
            },
            "example": "aa:bb:cc:dd"
          },
          {
            "name": "count",
            "type": "number",
            "title": {
              "en": "Count"
            },
            "example": "123"
          }
        ]
      }
    ]
  }
}